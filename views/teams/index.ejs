<%- include('../partials/header') %>

<table id ="info">
  <thead>
    <tr>
        <th>Club</th>
        <th>Country</th>
    </tr>
  </thead>
  <tbody>
    <% teams.forEach(function(t) { %>
       <tr>
        <td><%= t.club %></td>
        <td><%= t.country %></td>
        <td><a href="/teams/<%= t._id %>">DETAILS</a></td>
       </tr>
    <% }); %> 
  </tbody>
</table>

<%- include('../partials/footer') %>
